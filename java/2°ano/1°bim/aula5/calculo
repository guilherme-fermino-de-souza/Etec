public class Calculo {
	public boolean Calc(double angA, double angB, double angC) {
		
		double calc = angA + angB + angC;

	
			if(calc<180) {
				System.out.println("Não compõe um triângulo");
				return false;
			} else {
				System.out.println("Compõe um triângulo");
				return true;
			}
		}
		
